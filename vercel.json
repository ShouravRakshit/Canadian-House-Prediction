{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "client/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/get_location_names",
        "dest": "api/index.py"
      },
      {
        "src": "/api/predict_home_price",
        "dest": "api/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ]
  }